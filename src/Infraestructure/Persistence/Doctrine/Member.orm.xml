<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Domain\Entity\Member" table="member">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" length="255" nullable="true"/>
    <field name="websiteName" type="string" length="255" nullable="true"/>
    <field name="websiteUrl" type="string" length="1000" nullable="true"/>
    <field name="natsTrackingCodeForWebcams" type="string" length="20" nullable="true"/>
    <field name="natsTrackingCodeForMainPage" type="string" length="20" nullable="true"/>
    <field name="pathToCssDirectory" type="string" length="300" nullable="true"/>
    <field name="googleAnalyticsIdentifier" type="string" length="50" nullable="true"/>
  </entity>
</doctrine-mapping>
